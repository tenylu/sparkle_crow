{
  "name": "crowvpn",
  "version": "2.0.0",
  "description": "CrowVPN",
  "main": "./out/main/index.js",
  "author": "crowmesh",
  "homepage": "https://github.com/crowmesh/sparkle",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "prepare": "node scripts/prepare.mjs",
    "updater": "node scripts/updater.mjs",
    "checksum": "node scripts/checksum.mjs",
    "telegram": "node scripts/telegram.mjs",
    "artifact": "node scripts/artifact.mjs",
    "dev": "electron-vite dev",
    "preinstall": "npx only-allow pnpm -y",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "electron-vite build && electron-builder --publish never --win && pnpm checksum .exe .7z",
    "build:mac": "electron-vite build && electron-builder --publish never --mac && pnpm checksum .pkg .dmg",
    "build:linux": "electron-vite build && electron-builder --publish never --linux && pnpm checksum .deb .rpm .pkg.tar.xz AppImage"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
