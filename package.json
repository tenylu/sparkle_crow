    "iconv-lite": "^0.7.0",
    "leaflet": "^1.9.4",
    "react-leaflet": "^4.2.1",
    "webdav": "^5.8.0",
    "ws": "^8.18.3",
    "yaml": "^2.8.1",
    "zustand": "^5.0.8",
    "dompurify": "^3.1.6"
  },
  "devDependencies": {
    "@dnd-kit/core": "^6.3.1",
